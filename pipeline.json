{
  "pipeline": {
    "name": "square"
  },
  "transform": {
    "image": "erikreppel/test-pipeline",
    "cmd": [
      "sh"
    ],
    "stdin": [
      "python square.py /pfs/data /pfs/out"
    ]
  },
  "parallelism": "3",
  "inputs": [
    {
      "repo": {
        "name": "test_data"
      }
    }
  ]
}{
  "pipeline": {
    "name": "sum"
  },
  "transform": {
    "image": "erikreppel/test-pipeline-sum",
    "cmd": [
      "sh"
    ],
    "stdin": [
      "python sum.py /pfs/square /pfs/out"
    ]
  },
  "parallelism": "1",
  "inputs": [
    {
      "repo": {
        "name": "square"
      },
      "method": "reduce"
    }
  ]
}{
  "pipeline": {
    "name": "scale"
  },
  "transform": {
    "image": "erikreppel/scale-pipe",
    "cmd": [
      "sh"
    ],
    "stdin": [
      "echo $PACH_OUTPUT_COMMIT_ID",
      "python scale.py /pfs/square /pfs/sum /pfs/out"
    ]
  },
  "parallelism": "3",
  "inputs": [
    {
      "repo": {
        "name": "square"
      }
    },
    {
      "repo": {
        "name": "sum"
      },
      "method": {
        "partition": "file",
        "incrementality": true
      }
    }
  ]
}